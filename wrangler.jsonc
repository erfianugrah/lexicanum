{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "lexicanum",
  "compatibility_date": "2025-12-06",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page",
    "html_handling": "auto-trailing-slash"
  },
  "routes": [
    {
      "pattern": "erfi.dev",
      "custom_domain": true,
      "override_existing_origin": true,
      "override_existing_dns_record": true
    }
  ]
}
